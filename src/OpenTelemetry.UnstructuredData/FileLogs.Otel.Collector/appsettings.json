{
  "logConfigurations": [
    {
      "directories": [
        {
          "path": "..\\net8.0",
          "recursive": true,
          "searchPattern": "*.log"
        }
      ],
      "files": [
        "Log-information.log"
      ],
      "logLineStartRegex": "^(?<LogTimestamp>\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}) (?<LogType>\\w+)",
      "logLineStartTypeMatchingGroup": "",
      "logLineStartTimestampMatchingGroup": "",
      "logLineTimestampRegex": "^\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}",
      "logLineTimestampMatchingGroup": "LogTimestamp",
      "logLineTypeRegex": "^\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2} (?<LogType>\\w+)",
      "logLineTypeMatchingGroup": "LogType"
    }
  ]
}